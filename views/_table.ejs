<table class="table table-striped table-hover" id="toload">
    <thead>
        <tr>
            <!-- <th>Campus</th> -->
            <th>FirstName</th>
            <th>Surname</th>
            <th>Gender</th>
            <th>Nu</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        <% campus.forEach(student => { %>
            <tr>
                <!-- <td><%= student.Campus %></td> -->
                <td><%= student.firstName %></td>
                <td><%= student.surname %></td>
                <td><%= student.gender %></td>
                <td><%= student['Nu.'] %></td>
                <td><div class="dropdown dropend">
                     
                    <i style="font-size:24px; cursor: pointer;" class="fa" data-bs-toggle="dropdown">&#xf142;</i>
                     
                    <ul class="dropdown-menu">
                      <li><a class="dropdown-item" href="#">Update</a></li>
                      <li><a class="dropdown-item text-danger" href="#">Delete</a></li>
                    </ul>
                  </div>
                </td>
                 
            </tr>
        <% }) %>
    </tbody>
</table>